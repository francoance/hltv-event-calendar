{

    "manifest_version": 2,
    "name": "HltvEventCalendar",
    "version": "1.1",
  
    "description": "Add a button to the event page on HLTV to download an ICS file that contains all the matches.",
  
    "icons": {
      "48": "icons/icon.png",
      "96": "icons/icon_x2.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.hltv.org/events/*/matches"],
        "js": ["hltveventcalendar.js"]
      }
    ]
  
  }
  